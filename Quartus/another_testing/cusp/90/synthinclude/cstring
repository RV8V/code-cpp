#ifndef _CSTRING_HEADER
#define _CSTRING_HEADER

#include <cstddef>

#include <string.h>

namespace std 
{
	using ::memchr;
	using ::memcmp;
	using ::memcpy;
	using ::memmove;
	using ::memset;
	using ::strcat;
	using ::strncat;
	using ::strchr;
	using ::strrchr;
	using ::strcmp;
	using ::strncmp;
	using ::strcoll;
	using ::strcpy;
	using ::strncpy;
	using ::strcspn;
	using ::strerror;
	using ::strspn;
	using ::strlen;
	using ::strpbrk;
	using ::strstr;
	using ::strtok;
	using ::strxfrm;
}

#endif
