// cctype
//
// This header file is used when the Altera CusP compiler is compiling
// your source code to hardware.

#ifndef _CCTYPE_HEADER
#define _CCTYPE_HEADER

#include <ctype.h>

namespace std
{
	using ::isalnum;
	using ::isalpha;
	using ::iscntrl;
	using ::isdigit;
	using ::isgraph;
	using ::islower;
	using ::isprint;
	using ::ispunct;
	using ::isspace;
	using ::isupper;
	using ::isxdigit;
	using ::tolower;
	using ::toupper;
}
#endif
